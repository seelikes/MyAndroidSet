<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/el_item_bg"
    android:orientation="vertical">

    <TextView
        android:id="@+id/host_class"
        android:layout_width="match_parent"
        android:layout_height="64px"
        android:layout_marginStart="64px"
        android:layout_marginEnd="64px"
        android:singleLine="true"
        android:ellipsize="middle"
        android:textSize="36px"
        android:textColor="@color/title_text_color" />

    <TextView
        android:id="@+id/method"
        android:layout_width="match_parent"
        android:layout_height="64px"
        android:layout_marginStart="64px"
        android:layout_marginEnd="64px"
        android:textSize="36px"
        android:textColor="@color/title_text_color" />

    <TextView
        android:id="@+id/count"
        android:layout_width="match_parent"
        android:layout_height="64px"
        android:layout_marginStart="64px"
        android:layout_marginEnd="64px"
        android:textSize="36px"
        android:textColor="@color/title_text_color" />

    <TextView
        android:id="@+id/total_duration"
        android:layout_width="match_parent"
        android:layout_height="64px"
        android:layout_marginStart="64px"
        android:layout_marginEnd="64px"
        android:textSize="36px"
        android:textColor="@color/title_text_color" />

    <TextView
        android:id="@+id/longest_duration"
        android:layout_width="match_parent"
        android:layout_height="64px"
        android:layout_marginStart="64px"
        android:layout_marginEnd="64px"
        android:textSize="36px"
        android:textColor="@color/title_text_color" />

    <TextView
        android:id="@+id/shortest_duration"
        android:layout_width="match_parent"
        android:layout_height="64px"
        android:layout_marginStart="64px"
        android:layout_marginEnd="64px"
        android:textSize="36px"
        android:textColor="@color/title_text_color" />

    <TextView
        android:id="@+id/average_duration"
        android:layout_width="match_parent"
        android:layout_height="64px"
        android:layout_marginStart="64px"
        android:layout_marginEnd="64px"
        android:textSize="36px"
        android:textColor="@color/title_text_color" />

</LinearLayout>